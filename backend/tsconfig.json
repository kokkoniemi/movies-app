{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "target": "es6",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true
    },
    "lib": ["es2015"],
    "include": ["./src/**/*", "./.eslintrc.js"],
    "exclude": ["./node_modules/"]
}
